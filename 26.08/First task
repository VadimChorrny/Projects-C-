#include <iostream>
using namespace std;

void ArraysChange() {
    const int count = 10;
    int arr[count], a;
    for (int i = 0; i < count; i++)
    {
        cout << i + 1 << "/" << count << ": ";
        cin >> a;
        arr[i] = a;
        cout << arr[i] << " ";
    }
    cout << endl;
    int min = 0;
    int max = 0;
    for (int i = 0; i < count; i++)
    {
        if (arr[i] < arr[min])
            min = i;
        if (arr[i] > arr[max])
            max = i;
    }
    int tmp = arr[max];
    arr[max] = arr[min];
    arr[min] = tmp;
    for (int i = 0; i < count; i++)
    {
        cout << arr[i] << " ";
    }
}

int main()
{
    ArraysChange();
}
